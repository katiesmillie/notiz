           <li><a href="users/sign_in">login</a></li>
      </ul>
     <% end %>
  </div>
</div>
     

  <% if flash[:success].present? %>
    <div class="alert alert-success alert-green">
      <button type="button" class="close" data-dismiss="alert"></button>
      <%= flash[:success]%>
    </div>
  <% end %>

    <% if flash[:future].present? %>
    <div class="alert alert-success alert-green">
      <button type="button" class="close" data-dismiss="alert"></button>
      <%= flash[:future]%>
    </div>
  <% end %>




